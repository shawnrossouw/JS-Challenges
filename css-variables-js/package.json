{
  "name": "vader-lite",
  "version": "1.0.0",
  "description": "Parcel js and postcss lite workflow",
  "main": "index.js",
  "scripts": {
    "dev": "parcel index.html",
    "build": "parcel build index.html --out-dir production"
  },
  "keywords": [],
  "author": "Shawn Rossouw",
  "license": "ISC",
  "dependencies": {
    "postcss-nested": "^4.1.2"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "postcss-nested": {},
      "autoprefixer": {
